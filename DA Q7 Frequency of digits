//Program to print frequency of digits 0-9 from the given long string mixed with numbers and characters
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b,c,d,e,f,g,h,i,j;
    char s[1000];
    a=b=c=d=e=f=g=h=i=j=0; 
    scanf("%s",s);
    for(int k=0;k<strlen(s);k++)
    {
        if(s[k]=='0')
        a+=1;
        else if(s[k]=='1')
        b+=1;
        else if(s[k]=='2')
        c+=1;
        else if(s[k]=='3')
        d+=1;
        else if(s[k]=='4')
        e+=1;
        else if(s[k]=='5')
        f+=1;
        else if(s[k]=='6')
        g+=1;
        else if(s[k]=='7')
        h+=1;
        else if(s[k]=='8')
        i+=1;
        else if(s[k]=='9')
        j+=1;
    }
    printf("%d %d %d %d %d %d %d %d %d %d",a,b,c,d,e,f,g,h,i,j);
    return 0;
}

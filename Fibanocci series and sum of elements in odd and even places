//Program to find the fibanocci series till the entered number is reached and finding the sum of elements in odd and even position
#include <stdio.h>
int fib(int n){
    int test=1,a[256],i=0,j=1,k=2,x,oddsum=0,evensum=0;
    a[0]=0;
    a[1]=1;
    while(test){
        if(i!=n){
            x=i+j;
            a[k]=x;
            i=j;
            j=x;
            k++;
        }
        else
        test=0;
    }
    for(i=0;i<k-1;i++){
        if(i%2==0)
        oddsum+=a[i];
        else
        evensum+=a[i];
        if(i!=k-2)
        printf("%d ",a[i]);
        else
        printf("%d",a[i]);
    }
    printf("\nThe sum of elements in odd position is %d",oddsum);
    printf("\nThe sum of elements in even position is %d",evensum);
}
int main(){
    int n;
    scanf("%d",&n);
    fib(n);
    return 0;
}

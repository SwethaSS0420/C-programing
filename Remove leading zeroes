//Program to remove leading zeroes of a number
#include <stdio.h>
#include <string.h>
int main()
{
  char s[10],a;
  scanf("%s",s);
  int i;
  for(i=0;i<strlen(s);i++)
  {
    while(s[i]=='0')
    { 
      for(int j=i;j<strlen(s);j++)
      {
        s[j]=s[j+1];
      }
      s[strlen(s)='\0';
    }
    if(s[i]!='0')
    {
      break;
    }
  }
  printf("%s\n",s);
  return 0;
}    

//Sorting array
#include <stdio.h> 
int main(){
    int size,i,j,x;
    scanf("%d",&size); 
    int a[size];
    for (i=0;i<size;i++){
        scanf("%d",&a[i]);
    }
    for (i=0;i<size;i++){ 
        for (j=0; j<size;j++){
            if(a[i]<a[j]){
                x=a[i];
                a[i]=a[j];
                a[j]=x;
            }
        }
    }
    for(i=0;i<size;i++){
        printf("%d ",a[i]);
    }
    return 0;
}

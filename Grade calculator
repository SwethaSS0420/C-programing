//Calculate the grade for the mark entered
#include <stdio.h>
int main(){
    int a,b;
    scanf("%d", &a);
    if (a%10!=0)
        b=a/10;
    else
        b=(a/10)-1;
    
    switch(b){
        case 9:
        printf("O");
        break;
        
        case 8:
        printf("A");
        break;
        
        case 7:
        printf("B");
        break;
        
        case 6: case 5: case 4: case 3: case 2: case 1: case 0:
        printf("D");
        break;
        
        default:
        printf("Error. Not in scope.");
    }
    return 0;
}

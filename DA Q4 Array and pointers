//Find the sum of array element using malloc and pointers
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,*ptr,sum=0;
    scanf("%d",&n);
    ptr=(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    {
        scanf("%d",(ptr+i));
        sum+=*(ptr+i);
    }  
    printf("%d",sum);
    return 0;
}
